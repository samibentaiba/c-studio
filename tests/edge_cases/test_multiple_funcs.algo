// Test: Multiple functions calling each other
ALGORITHM MultipleFunctions
VAR
    result : INTEGER

FUNCTION Add(a : INTEGER, b : INTEGER) : INTEGER
BEGIN
    RETURN(a + b)
END

FUNCTION Multiply(a : INTEGER, b : INTEGER) : INTEGER
BEGIN
    RETURN(a * b)
END

FUNCTION Square(n : INTEGER) : INTEGER
BEGIN
    RETURN(Multiply(n, n))
END

FUNCTION SumOfSquares(a : INTEGER, b : INTEGER) : INTEGER
BEGIN
    RETURN(Add(Square(a), Square(b)))
END

FUNCTION Cube(n : INTEGER) : INTEGER
BEGIN
    RETURN(Multiply(n, Square(n)))
END

BEGIN
    PRINT("=== Multiple Function Tests ===")
    
    // Simple calls
    PRINT("2 + 3 =", Add(2, 3))
    PRINT("4 * 5 =", Multiply(4, 5))
    
    // Nested calls
    PRINT("5^2 =", Square(5))
    PRINT("3^3 =", Cube(3))
    
    // Complex nested
    PRINT("3^2 + 4^2 =", SumOfSquares(3, 4))
    
    // Chain of calls
    result <- Add(Multiply(2, 3), Multiply(4, 5))
    PRINT("(2*3) + (4*5) =", result)
    
    // Edge: zero
    PRINT("0^2 =", Square(0))
    PRINT("1^3 =", Cube(1))
END.
